<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.motorplace">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".activitys.TelaRecuperarSenha"
            android:parentActivityName=".activitys.TelaDeLogin"
            />
        <activity android:name=".activitys.TelaHomeAdm" />
        <activity android:name=".activitys.TelaHomeCliente" android:theme="@style/appbar"/>
        <activity
            android:name=".activitys.TelaCadastroCarro"
            android:parentActivityName=".activitys.TelaCadastroUsuario" />
        <activity
            android:name=".activitys.TelaCadastroUsuario"
            android:parentActivityName=".activitys.TelaDeEntradaActivity" />
        <activity
            android:name=".activitys.TelaDeLogin"
            android:parentActivityName=".activitys.TelaDeEntradaActivity" />
        <activity android:name=".activitys.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activitys.TelaDeEntradaActivity" />
    </application>

</manifest>